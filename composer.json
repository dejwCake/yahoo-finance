{
    "name": "dejwcake/yahoo-finance",
    "description": "Yahoo Finance Client",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "authors": [
        {
            "name": "David BÄ›hal",
            "email": "behal.david@gmail.com",
            "homepage": "https://www.dejw.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "nesbot/carbon": "^2.66 || ^3.0",
        "php-http/discovery": "^1.18",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^11.5 || ^12.0",
        "symfony/var-dumper": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "DejwCake\\YahooFinance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DejwCake\\YahooFinance\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DejwCake\\YahooFinance\\YahooFinanceServiceProvider"
            ]
        }
    }
}
